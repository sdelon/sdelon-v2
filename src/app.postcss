/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

.container {
    @apply max-w-7xl mx-auto px-4 sm:px-6;
}

.svg-shadow {
    filter: drop-shadow( 0px 2px 4px rgba(0, 0, 0, .25));
}

.svg-link:hover {
    filter: drop-shadow( 0px 4px 4px rgba(0, 0, 0, .5));
}